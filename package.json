{
  "name": "pages",
  "version": "3.1.0",
  "description": "sources of amazingandyyy.com",
  "scripts": {
    "dev": "parcel ./src/index.html --open",
    "build": "npm run cleanup && parcel build ./src/index.html -d docs && npm run copy",
    "cleanup": "rm -rf docs",
    "copy": "npm run copy-static && npm run copy-cname",
    "copy-static": "cp -rf src/statics docs/",
    "copy-cname": "mv docs/statics/CNAME docs/CNAME"
  },
  "repository": "https://github.com/amazingandyyy/pages",
  "author": "amazingandyyy",
  "license": "MIT",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.1.0",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^3.16.1",
    "eslint-plugin-react": "^6.10.0",
    "parcel-bundler": "^1.12.3",
    "sass": "^1.22.7"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "rules": {
      "no-const-assign": "warn",
      "no-this-before-super": "warn",
      "no-undef": "warn",
      "no-unreachable": "warn",
      "no-unused-vars": "warn",
      "constructor-super": "warn",
      "valid-typeof": "warn",
      "react/jsx-uses-react": 1
    },
    "plugins": [
      "react"
    ]
  }
}
